    "eslint-plugin-react": "^7.5.1",
    "exec-series": "^1.0.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.10",
    "image-webpack-loader": "^4.2.0",
    "kotatsu": "git+https://github.com/robindemourat/kotatsu.git",
