{
  "name": "dicto",
  "version": "0.1.0-alpha",
  "description": "transcribe > annotate > remix > publish media contents",
  "author": "Robin de Mourat",
  "repository": "https://github.com/dictoapp/dicto",
  "homepage": "https://dictoapp.github.io/dicto",
  "bugs": {
    "url": "https://github.com/dictoapp/dicto/issues"
  },
  "license": "AGPL-3.0",
  "main": "main_process.js",
  "scripts": {
    "comb": "csscomb app/src",
    "build:web": "npm run private:build:bundles;webpack ./app/src/main.js -o docs/build/bundle.js --config ./webpack.web.prod.js --babel --progress;cp app/*.png docs/;cp app/loading.gif docs/loading.gif; node prepareWebProd;",
    "dev:electron": "NODE_ENV=development npm-run-all --parallel private:bundle private:serve",
    "dev:web": "kotatsu serve --config ./webpack.web.dev.js --progress --babel --index ./app/index.html --public ./app/ ./app/src/main.js",
    "lint": "eslint app/src; eslint services",
    "lint:fix": "eslint app/src --fix; eslint services --fix",
    "pack": "npm run private:build:bundles;npm run private:compile;run-s private:build:all",
    "pack:linux": "run-s private:clean private:compile private:build:linux",
    "pack:mac": "run-s private:clean private:compile private:build:mac",
    "pack:old": "run-s private:clean private:compile private:build:all",
    "pack:win": "run-s private:clean private:compile private:build:win",
    "private:add:docs": "git add docs",
    "private:build-corpus-player": "webpack ./app/src/components/CorpusPlayer/Standalone.js -o ./app/bundles/corpus-player/bundle.js  --progress --config ./webpack.web.prod.js --babel",
    "private:build-montage-player": "webpack ./app/src/components/MontagePlayer/Standalone.js -o ./app/bundles/montage-player/bundle.js  --progress --config ./webpack.web.prod.js --babel",
    "private:build:all": "npm run private:build:icons;electron-builder -mwl",
    "private:build:bundles": "npm run private:build-montage-player;npm run private:build-corpus-player",
    "private:build:icons": "./node_modules/.bin/electron-icon-maker --input=./assets/icon.png --output=./assets/;node flattenIcons",
    "private:build:linux": "electron-builder --linux",
    "private:build:mac": "electron-builder --mac",
    "private:build:win": "electron-builder --win",
    "private:bundle": "webpack --config ./webpack.electron.dev.js",
    "private:clean": "rimraf build",
    "private:compile": "webpack --config ./webpack.electron.prod.js",
    "private:serve": "electron ."
  },
  "precommit": [
    "lint",
    "build:web",
    "private:add:docs"
  ],
  "build": {
    "productName": "dicto",
    "appId": "com.electron.dicto",
    "linux": {
      "target": "tar.gz",
      "synopsis": "transcribe > annotate > remix > publish media contents",
      "category": "AudioVideo"
    },
    "mac": {
      "category": "public.app-category.video",
      "type": "development"
    },
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    }
  },
  "engines": {
    "node": ">=9.2.0",
    "npm": ">=6.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.23.0",
